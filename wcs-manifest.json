      "organizationUrl": "https://noi.bz.it"
    }
  ],
  "authors": [
    {
      "name": "Rudi Thoeni",
      "email": "r.thoeni@noi.bz.it",
      "organization": "NOI S.p.A",
      "organizationUrl": "https://noi.bz.it"
    }
  ],
  "image": "wcs-logo.png",
  "searchTags": [
    "map"
  ],
  "dist": {
    "basePath": "dist",
    "files": [
      "webcomp-accommodations.min.js"
    ],
    "scripts": [
    ]
  },
  "configuration": {
    "tagName": "webcomp-accommodations",
    "options": [
      {
        "key": "centermap",
        "type": "text",
        "label": "latitude and longitude: ex. 46.7728692,10.7916716",
        "required": false,
        "options": {
          "default": "46.7728692,10.7916716"
        }
      },
      {
        "key": "zoommap",
        "type": "text",
        "label": "Zoom level of the map example 10",
        "required": false,
        "options": {
          "default": "10"
        }
      },
      {
        "key": "pagesize",
        "type": "text",
        "label": "How many Accommodations should be retrieved",
        "required": false,
        "options": {
          "default": "500"
        }
      },
      {
        "key": "source",
        "type": "multiselect",
        "label": "Sources of the Accommodations",
        "required": false,
        "options": {
          "values": [
            "lts",
            "discoverswiss"
          ]
        }
      }
    ]
  }
}

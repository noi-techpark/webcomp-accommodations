  "title": "Accommodations Visualization",
  "shortName": "Accommodations",
  "description": "A simple map that shows accommodations available from Open Data Hub",
  "descriptionAbstract": "A simple map that shows accommodations available from Open Data Hub",
  "license": "AGPL-3.0-or-later",
  "repositoryUrl": "https://github.com/noi-techpark/webcomp-accommodations.git",
  "copyrightHolders": [
    {
      "name": "NOI",
